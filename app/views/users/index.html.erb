<h1>Users</h1>
<% @users.zip(@books) do |user, book| %>
<p>image</p>
<%= link_to user_path(user.id) do %>
<%=attachment_image_tag user, :profile_image, :fill, 60, 60, fot: "jpg", fallback: "no_image.jpg" %>
<% end %>
<p>name</p>
<%= user.name %>
<p><%= link_to "show", book_path(book.id) %></p>
<% end %>